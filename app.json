{
  "expo": {
    "name": "Book Search",
    "slug": "Book-Search-App",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#202020"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.booksearch.app"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.booksearch.app"
    }
  }
}